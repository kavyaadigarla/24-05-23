#include <stdio.h>

int main() {
    int arr[] = {10, 20, 10, 20, 30, 20, 40};
    int size ;
    int element, frequency = 0;

    printf("Enter the element to find its frequency: ");
    scanf("%d", &element);

    for (int i = 0; i < size; i++) {
        if (arr[i] == element) {
            frequency++;
        }
    }

    printf("%d times\n", frequency);

    return 0;
}
